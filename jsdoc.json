{
  "source": {
    "include": ["README.md", "src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "default": {
      "includeDate": false,
      "outputSourceFiles": false
    },
    "cleverLinks": true,
    "monospaceLinks":true
  },
  "opts": {
    "recurse": true,
    "template": "node_modules/docdash",
    "tutorials": "./docs/tutorials",
    "destination": "./docs/"
  },
  "docdash": {
    "sectionOrder": [
      "Tutorials",
      "Modules",
      "Classes",
      "Externals",
      "Events",
      "Namespaces",
      "Mixins",
      "Interfaces"
    ],
    "typedefs": true
  }
}